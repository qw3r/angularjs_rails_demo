source 'http://rubygems.org'

gem 'rails'
gem 'paperclip'
gem 'jquery-rails'

gem 'sqlite3'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails', '~> 3.2.6'
  gem 'coffee-rails', '~> 3.2.2'
  gem 'uglifier'
end

group :development, :test do
  gem 'rspec-rails'
  gem 'jasmine'
  gem 'jasminerice'
end

group :test do
  gem 'capybara'
  gem 'launchy'
  gem 'database_cleaner', '1.0.1'
end

